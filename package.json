{
  "name": "shippingmanager-copilot",
  "version": "0.1.7.1",
  "description": "Standalone web interface for Shipping Manager chat and game management",
  "main": "app.js",
  "bin": "app.js",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "start": "node helper/launcher/nodejs/index.js",
    "start:headless": "node helper/launcher/nodejs/headless.js",
    "docs": "jsdoc -c jsdoc.json",
    "prepare": "husky",
    "build": "node build/build.js",
    "build:sea": "node build/build-sea.js",
    "build:sea:mac": "node build/build-sea.js --platform=mac",
    "build:sea:linux": "node build/build-sea.js --platform=linux",
    "build:package": "node build/build-package.js",
    "build:installer": "node build/build-installer.js",
    "build:all": "npm run build:sea && npm run build:package && npm run build:installer",
    "lint": "eslint .",
    "lint:css": "stylelint \"public/css/**/*.css\" \"launcher/dialogs/**/*.css\"",
    "security": "npm run lint"
  },
  "dependencies": {
    "@primno/dpapi": "^2.0.1",
    "axios": "^1.13.1",
    "better-sqlite3": "^12.5.0",
    "cron": "^4.3.3",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "keytar": "^7.9.0",
    "koffi": "^2.14.1",
    "marked": "^17.0.1",
    "multer": "^2.0.2",
    "node-forge": "^1.3.1",
    "selenium-webdriver": "^4.27.0",
    "systray2": "^2.1.4",
    "undetected-chromedriver-js": "^1.2.2",
    "unzipper": "^0.12.3",
    "validator": "^13.15.20",
    "webview-nodejs": "^0.4.3",
    "winston": "^3.18.3",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "archiver": "^7.0.1",
    "docdash": "^2.0.2",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.0",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-no-unsanitized": "^4.1.4",
    "eslint-plugin-pii": "^1.0.2",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "husky": "^9.1.7",
    "jsdoc": "^4.0.5",
    "leaflet": "^1.9.4",
    "leaflet-environmental-layers": "^2.4.9",
    "leaflet.markercluster": "^1.5.3",
    "lightweight-charts": "^5.0.9",
    "page-flip": "^2.0.7",
    "postject": "^1.0.0-alpha.6",
    "react-pageflip": "^2.0.3",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1"
  }
}
